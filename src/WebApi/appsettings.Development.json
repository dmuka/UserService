{
  "DbConnections": {
    "Postgres": {
      "Host": "localhost",
      "Port": 5432,
      "Database": "postgres",
      "Username": "postgres",
      "Password": 123456789
    }
  },
  "Jwt": {
    "Secret": "secretsecretsecretsecretsecretsecret",
    "Issuer": "issuer",
    "Audience": "audience",
    "ExpirationInMinutes": 20
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
